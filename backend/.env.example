# Network Rail Darwin Push Port Credentials
# Get from https://datafeeds.networkrail.co.uk/ → "My Feeds" → "Darwin"

# Darwin STOMP/OpenWire Connection (Primary for real-time updates)
DARWIN_PUSH_PORT_USERNAME=your-darwin-username-here
DARWIN_PUSH_PORT_PASSWORD=your-darwin-password-here
DARWIN_MESSAGING_HOST=darwin-dist-YYYY-XX.nationalrail.co.uk
DARWIN_STOMP_PORT=61613
DARWIN_OPENWIRE_PORT=61616
DARWIN_TOPIC_LIVE=darwin.pushport-v16
DARWIN_TOPIC_STATUS=darwin.status

# Darwin FTP Access (for historical data)
DARWIN_FTP_HOST=darwin-dist-YYYY-XX.nationalrail.co.uk
DARWIN_FTP_USERNAME=ftpuser
DARWIN_FTP_PASSWORD=your-ftp-password-here

# Darwin S3 Bucket Access (for timetable files)
DARWIN_S3_BUCKET=darwin.xmltimetable
DARWIN_S3_PREFIX=PPTimetable/
DARWIN_S3_ACCESS_KEY=your-s3-access-key
DARWIN_S3_SECRET_KEY=your-s3-secret-key
DARWIN_S3_REGION=eu-west-1

# Backend Configuration
BACKEND_PORT=4000
NODE_ENV=development
FRONTEND_URL=http://localhost:3000

# Frontend Configuration
NEXT_PUBLIC_API_URL=http://localhost:4000
